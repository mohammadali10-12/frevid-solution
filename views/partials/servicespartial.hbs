<section class="service_cs">
    <div class="texts" style="text-align: center;padding:80px">

        <span>Our-Services</span>
    </div>

    <div class="service-flex">


        {{#each data}}

        <div class="service-box">
            <div class="card container" style="height:455px">
                <a href="{{this.link}}">
                    <img src="{{this.imgUrl}}" class="service-img" alt="...">
                </a>
                <div class="card-body p-3 center ">
                    <h4>{{this.title}}</h4>
                    <p class="card-text" style="text-align:center;">{{this.subTitle}}</p>
                </div>
            
            </div>

        </div>
        {{/each}}

    </div>

</section>




